mixin search-result(items)
  each item in items
    .search-result__item
      .search-result__wrap
        if (typeof(item.image1) !== 'undefined')
          .search-result__thumb
            +link('Opening in a new window', item.href)(target="_blank").search-result__thumb-link
              picture.picture-block
                source(srcset!=item.image1, media="(min-width: 1240px)")
                source(srcset!=item.image2, media="(min-width: 480px)")
                +img(item.image1, item.alt)(class=item.classShadow ? "all-document__img " + item.classShadow : "search-result__img")
        .search-result__content
          .tag-list
            span(class=item.classTagColor ? "tag-list__item " + item.classTagColor : "tag-list__item") #{item.tagName}
          +link('Opening in a new window', item.href, item.title)(target="_blank").search-result__title
          if (typeof(item.desc) !== 'undefined')
            p.search-result__desc #{item.desc}
          .search-result__breadcrumb
            - var leng = item.breadcrumb.length
            each breadcrumb, key in item.breadcrumb
              if ((leng - key) !== 1)
                span.breadcrumb__item
                  span.breadcrumb--text!=breadcrumb.title
              else
                span.breadcrumb__item
                  span.breadcrumb--text-active!=breadcrumb.textActive
mixin modal(item)
  - if (typeof item.modalLabel === 'undefined') item.modalLabel = 'modalLabel'
  .modal.fade(id= 'modalTarget', tabindex = '-1', aria-labelledby= item.modalLabel, role="dialog", data-modal)
    .modal-dialog
      .modal-content
        .modal-header
          button(type="button", data-dismiss="modal", aria-label="Close").close
            span(aria-hidden="true") &times;
        .modal-body
          p.modal-notice!=item.modalNotice
          input(type="checkbox", name="confirm", value="confirm", id="confirm").checkbox-confirm
          label(for="confirm").description
            span.confirm-text!=item.modalConfirm
          span.message-checkbox Please check to checkbox!
        .modal-footer
          a(href="#", title="confirm", data-link-ex= 'false').btn-confirm CONFIRM
