mixin breadcrumb(items)
  nav.breadcrumb(data-sticky-nav)&attributes(attributes)
    .container
      +link('Logo').site-logo
        +img('assets/img/site-logo.png', 'Logo')
      ul
        - var leng = items.length
        each item, key in items
          if ((leng - key) !== 1)
            li.breadcrumb__item
              +link(item.title, item.href).breadcrumb__link
          else
            li.breadcrumb__item
              span.breadcrumb__text!=item.textActive
      +link('Back to top').back-to-top(data-scroll-to)
        span.ico-arrow-up
          span.sr-only arrow up icon

mixin socialList(items)
  ul.social-list&attributes(attributes)
    each item in items
      li.social-list__item
        +link(item.title, item.href)(target="_blank", class=item.class)
          span(class=item.classIcon)
            span.sr-only!= item.textLink

mixin newsItem(items)
  .news&attributes(attributes)
    each item in items
      +link(item.linkImage, item.hrefImage)(class=item.class)
        +picture([
          {
            src: item.srcset1,
            breakpoint: 1240
          },
          {
            src: item.srcset2
          }
        ], item.alt).picture-block
      .news__info
        .tag.news__tag
          each tag in item.tags
            span.tag-list__item!=tag.tagtext
        h3.news__title
          +link(item.title, item.href).news__title-link
        .news__description
          p!=item.decs
        .news__social-time
          time(datetime!=item.dateTime).news__date!=item.newstime
          +socialList ([
            {title: 'Facebook', href: 'https://www.facebook.com/sharer/sharer.php?u=https%3A//www.google.com', textLink: 'Unibail Facebook Social', class: 'social-list__link', classIcon: 'social-icon ico-facebook'},
            {title: 'Twitter', href: 'https://twitter.com/home?status=https%3A//www.google.com', textLink: 'Unibail Twitter Social', class: 'social-list__link', classIcon: 'social-icon ico-twitter'},
            {title: 'Linkedin', href: 'https://www.linkedin.com/shareArticle?mini=true&url=https%3A//www.google.com&title=Google&summary=&source=', textLink: 'Unibail Twitter Social', class: 'social-list__link', classIcon: 'social-icon ico-in'}
          ])

