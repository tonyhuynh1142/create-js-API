mixin custom-radio(obj)
  div(class=obj.classParent ? "custom-radio " + obj.classParent : "custom-radio")
    input(
          type = "radio"
          name = (obj.name||obj.id)
          class = obj.classInput ? obj.classInput : "custom-radio__controll"
          value = obj.value ? obj.value : null
          id = obj.id
          checked = obj.checked ? obj.checked : null
        )
    if (obj.hasIcon)
      label(for=obj.id, class="custom-radio__label custom-radio__label--location")
        span(class=obj.iconLabel ? "ico-location "+ obj.iconLabel : "ico-location")
          span.hidden marker-icon
        | #{obj.radiolabel||""}
    else
      label(for=obj.id, class="custom-radio__label") #{obj.radiolabel||""}

mixin titleBlock(obj)
  div(class=obj.classParent ? "title-block " + obj.classParent : "title-block")&attributes(attributes)
    if(obj.hasTitle !== false)
      h2(class=obj.classHeading ? "title-block__title " + obj.classHeading : "title-block__title")!=obj.titleText
    if(obj.hastext)
      span(class=obj.classSubHeading ? "title-block__desc " + obj.classSubHeading : "title-block__desc")!=obj.text

