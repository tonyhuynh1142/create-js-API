mixin all-document(items)
  each item in items
    .all-document__item
      .container
        if (typeof(item.image1) !== 'undefined')
          .all-document__thumb
            +link(item.title, item.href)(target="_blank", data-toggle="modal", data-target= '#modalTarget', data-confirm, data-download-file='true').all-document__thumb-link
              +picture([
                {
                  src: item.image1,
                  breakpoint: 1240
                },
                {
                  src: item.image2
                }
              ], item.alt, 'all-document__img').picture-block
        .all-document__content(class=item.class)
          .tag-list
            span.tag-list__item #{item.tag}
          +link(item.title, item.href)(target="_blank", data-toggle="modal", data-target= '#modalTarget', data-confirm, data-download-file='false').all-document__title
          time.date(datetime=item.dateTime)
            | #{item.date}
            sup th
            | &nbsp;#{item.year}
        .all-document__download-wrap
          +link('Download .pdf 125 KO', item.download_link)(target="_blank", download=item.downloadName, data-toggle="modal", data-target= '#modalTarget', data-confirm, data-download-file='true').all-document__download-link
            span.ico-file
              span.sr-only File icon
            span.all-document__download-text Download .pdf 125 KO

